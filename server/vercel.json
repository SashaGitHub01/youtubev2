{
   "version": 2,
   "builds": [
      {
         "src": "dist/index.js",
         "use": "@vercel/node",
         "config": {
            "includeFiles": [
               "dist/**"
            ]
         }
      }
   ],
   "rewrites": [
      {
         "source": "/uploads/**/*",
         "destination": "dist/index.js"
      },
      {
         "source": "/**/*",
         "destination": "dist/index.js"
      }
   ]
}