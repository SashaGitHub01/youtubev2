{
   "version": 2,
   "builds": [
      {
         "src": "dist/index.js",
         "use": "@vercel/node",
         "config": {
            "includeFiles": [
               "dist/**"
            ]
         }
      }
   ],
   "rewrites": [
      {
         "source": "/uploads/:folder*",
         "destination": "/:folder*"
      }
   ],
   "redirects": [
      {
         "source": "/(.*)",
         "destination": "dist/index.js"
      }
   ]
}